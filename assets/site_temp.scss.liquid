.e-scale{
	.dropdown-menu{
		@include scale(0);
		@include transition(all 0.3s ease 0s);
	}
	&.popup-over:hover,&.open{
		.dropdown-menu{
			@include scale(1);
			@include transition(all 0.3s ease 0s);
		}
	}	
}
.e-translate-left{ 
	.popup-content{
		@include opacity(0);
		@include translate(-200px, 0px);
		@include transition(all 0.3s ease 0s);
	}
	&.popup-over:hover{ 
		.popup-content{ 
			@include opacity(1);
			@include translate(0, 0);
			@include transition(all 0.3s ease 0s);
		}
	}
}
.e-translate-right{ 
	.popup-content{
		@include opacity(0);
		@include translate(200px, 0px);
		@include transition(all 0.3s ease 0s);
	}
	&.popup-over:hover{ 
		.popup-content{ 
			@include opacity(1);
			@include translate(0, 0);
			@include transition(all 0.3s ease 0s);
		}
	}
}
.e-translate-top{ 
	.popup-content{
		@include opacity(0);
		@include translate(0, 200px);
		@include transition(all 0.3s ease 0s);
	}
	&.popup-over:hover{ 
		.popup-content{ 
			@include opacity(1);
			@include translate(0, 0);
			@include transition(all 0.3s ease 0s);
		}
	}
}
.e-translate-down{
	.popup-content{
		@include opacity(0);
		height: 0;
		@include transition(all 0.3s ease 0s);
	}
	&.popup-over:hover{ 
		.popup-content{ 
			@include opacity(1);
			height: auto;
			@include transition(all 0.3s ease 0s);
		}
	}
}
.effect {
