    @include transition(all 450ms cubic-bezier(0.57,.06,.05,.95));
    @if (lightness( $border-color ) > 40) {background-color: #d3d3d3;}
    @else {background-color: #333333;}
}
@include at-query($min, $postSmall){
    .is-visible ~ .ajaxcartOverlay{visibility: visible;
        @include transition(all 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95));
        @if (lightness( $border-color ) > 40){opacity: 0.8;}
        @else{opacity: 0.95;}
        .lt-ie9 & {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";}
    }
}
.ajaxcartModal .ajaxcartContent {@include transform(scale(0.9));opacity: 0;}
.ajaxcartModal.is-visible .ajaxcartContent{@include transform(scale(1));opacity: 1;}
.ajaxcartModal{
  	@include at-query ($max, $medium) {
    	input[type="text"] {border-color: $border-color;}
    	.ajaxcartQtyAdjust {opacity: 1;}}}
.sprite {display: block;text-indent: -9999px;background-repeat: no-repeat;background-position: 0 0;}
.ajaxcartClose {
    @extend .sprite;
    position: absolute;top: 20px;right: 20px;opacity: 0;visibility: hidden;border: 0 none;width: 30px;height: 30px;cursor: pointer;
    background: {
      	color: transparent;
      	position: center center;
      	image: url(data:image/png;base64,)
    }z-index: 999999999;
    @include transition(all 450ms cubic-bezier(0.57,.06,.05,.95));
    .is-visible & {opacity: 0.8;visibility: visible;@include transition(opacity 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95) 200ms);}
    &:hover {opacity: 1;}
}
