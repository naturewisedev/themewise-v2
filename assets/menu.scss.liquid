#apollo-menu{
    font-family: $megamenu-font-family;
    background: $megamenu-bg-outsite;
    color: $megamenu-color;
  	padding: 0px;
    margin: 0px;
}
.apollo-megamenu{
	ul, ol {
        list-style: outside none none;
        margin: 0px;
        padding: 0px;
    }
}
#apollo-top-menu{
    padding: 0px;
    display: block;
    text-align: center;
    background-color: $megamenu-bg;
}
#apollo-menu{
    .navbar{
        background: $megamenu-bg;
        border: 0;
        margin: 0;
        padding: 0;
        @include rounded-corners(0);
    }
    /* menu level 1 */
    .navbar-nav{
        > li {
            margin:0px ;
            &.full-width {
                position: static;
                > .dropdown-menu  {
                    width: 100% !important;
                }
            }
            &.aligned-center {
                > .dropdown-menu  {
                    left: 50%;
                    @include translate(-50%, 0);
                }
            }
            > a{
                position: relative;
              	padding: $megamenu-parent-padding;              
                font-weight: $megamenu-font-weight;
              	font-size: $megamenu-parent-font-size;
                text-transform:  $megamenu-parent-text-transform;
                background-color: transparent;
              	color: $megamenu-link-color;
                @include transition(all 350ms ease-out);
                &:hover,&:focus,&:active{                   
                    color: $megamenu-link-hover-color;              
                    background-color: $megamenu-link-bg-hover-color;
                }
                &:after {
                  	content: "";
                    background: $theme-color-default none repeat scroll 0 0;                    
                    height: 0;
                    left: 0;
                    position: absolute;
                    bottom: 0;
                    @include transition(all 200ms ease-in-out 0s);
                    width: 100%;
                    z-index: 1;                  
                }
            }
            &.dropdown:hover{
              	>a{
                  	&:after{
                    	height: 3px;
                    }
                }
            }
            .caret{
                width: 10px;
              	border: 0px;
              	&:before{
                	content: "\f107";
                  	font-family: $font-icon;
                  	position: absolute;
                  	top: 12px;
                  	right: 15px;
                }
            }
        }       
    }
    /* level 2 */
    .dropdown-menu{         
        top: 150%;  
        left: 0px;
