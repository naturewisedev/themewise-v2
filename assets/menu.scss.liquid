$vermenu-link-hover-color: {{ settings.verticalmenu_linkhovercolor }};
$vermenu-link-bg-hover-color:  {{ settings.verticalmenu_linkhoverbgcolor }};
$vermenu-sub-text-color:   {{ settings.verticalmenu_textcolorsub }};
$vermenu-sub-link-color:   {{ settings.verticalmenu_linkcolorsub }};
$vermenu-sub-link-hover-color: {{ settings.verticalmenu_linkhoversub }};

$vermenu-font-family:                  $font-family-tertiary;
$vermenu-link-font-family:             $font-family;
$vermenu-font-weight:                  normal;
