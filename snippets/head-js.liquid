{% include 'oldIE-js' %}
{{ '//code.jquery.com/jquery-1.11.3.min.js' | script_tag }}
{% if template contains 'customers' %}
	{{ 'shopify_common.js' | shopify_asset_url | script_tag }}
{% endif %}
{{ 'api.jquery.js' | shopify_asset_url | script_tag }}
{{ 'option_selection.js' | shopify_asset_url | script_tag }}
{{ 'global.js' | asset_url | script_tag }}
{% assign pagewishlist = pages[settings.theme_wishlist_page] %}
<script>
	window.money_format = "{{shop.money_with_currency_format}}";
  	window.shop_currency = "{{ shop.currency }}";
	window.file_url = "{{''|file_url}}";
  	window.asset_url = "{{'Apollo'|asset_url|remove:"Apollo"}}";
    window.wishlist_url = "{{ pagewishlist.url }}";
    window.animated = {{ settings.animated_enable }};
    window.wishlist_enable = {{ settings.theme_wishlist_enable }};
    window.quickview_enable = {{ settings.theme_quickview_enable }};
    window.multiple_filter_sidebar_enable = {{ settings.use_multiple_filter }};
</script>